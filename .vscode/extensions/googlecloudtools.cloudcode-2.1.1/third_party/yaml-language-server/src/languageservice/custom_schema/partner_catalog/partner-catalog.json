{
  "$schema": "./catalog-schema.json",
  "schemas": [
    {
      "name": "Google Cloud Workflows",
      "description": "Google Cloud Workflows configuration file",
      "fileMatch": [
        "workflows.json",
        "workflows.yaml",
        "workflows.yml",
        "*.workflows.json",
        "*.workflows.yaml",
        "*.workflows.yml"
      ],
      "schemaFile": "workflows.json"
    },
    {
      "name": "Google Cloud Code source protect",
      "description": "Source protect policy configuration",
      "fileMatch": ["sp.yaml", "*.sp.yaml"],
      "schemaFile": "sourceprotectv1alpha1.json"
    }
  ]
}
