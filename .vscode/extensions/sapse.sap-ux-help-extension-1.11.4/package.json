{
	"name": "sap-ux-help-extension",
	"displayName": "SAP Fiori Tools - Guided Development",
	"description": "Allows you to access how-to guides and tutorials to implement SAP Fiori elements features using a guided approach.",
	"icon": "media/Guided-Development.png",
	"publisher": "SAPSE",
	"version": "1.11.4",
	"bugs": {
		"url": "https://ask.sapfioritools.com"
	},
	"repository": {
		"type": "git",
		"url": "private"
	},
	"engines": {
		"vscode": "^1.73.1"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"SAP",
		"SAP Fiori elements",
		"Help",
		"OData",
		"CDS"
	],
	"main": "./dist/extension-min.js",
	"private": false,
	"scripts": {
		"pre-commit": "lint-staged --quiet",
		"clean:dist": "rimraf ./dist ./generators *.tsbuildinfo",
		"clean": "rimraf ./dist ./reports *.tgz *.vsix ./.webpack_cache ./.vscode-test",
		"build": "npm-run-all -l -s clean build:compile build:extension -p build:copyfiles build:copyGuides build:copyFioriAI",
		"build:compile": "tsc --build tsconfig.build.json --dry",
		"build:extension": "node esbuild.js",
		"build:copyfiles": "copyfiles --flat ./src/assets/icons/* ./dist/assets/icons",
		"build:copyGuides": "copyfiles --up 5 ../core/dist/guidedDev/data/guides/**/images/** ./dist/data",
		"build:copyFioriAI": "copyfiles --up 5 ../../fiori-ai/ide-extension-lib/dist/* ../../fiori-ai/ide-extension-lib/dist/**/* ./dist/fiori-ai",
		"vscode:prepublish": "yarn run build",
		"watch": "npm-run-all -l -s clean build:compile build:copyfiles build:copyGuides build:copyFioriAI watch:extension",
		"watch:extension": "node esbuild.js --watch --minify=false --sourcemap=inline",
		"test": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --config='jest-unit.config.js'  && node ../../../scripts/scov.js reports/test/unit/coverage/lcov.info",
		"test:integration-disabled": "cross-env CODE_VERSION=1.53.2 CODE_TESTS_WORKSPACE=./test/int/input/ovp CODE_TESTS_PATH=./dist/test/int CODE_DISABLE_EXTENSIONS=true TOOL_SUITE_TIMEOUT=180000 node ./../../tools/jest-runner-vscode/dist/test.js",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:summary": "eslint . --ext .ts,.tsx -f summary",
		"lint:fix": "eslint --fix",
		"lint:fix:all": "eslint . --ext .ts,.tsx --fix",
		"lint:report": "eslint . --ext .ts,.tsx -f multiple ",
		"disable-test:e2e": "npm-run-all test:e2e:set test:e2e:export-guide",
		"test:e2e:set": "mkdirp reports/test/e2e",
		"test:e2e:export-guide": "yarn --cwd ../../../test/e2e test:e2e:help:export-guide",
		"format:fix": "prettier --write --ignore-path ../../../.prettierignore",
		"format:fix:all": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,dist,typings,node_modules}/**' '!**/*.{svg,png,jpg,xml}' --ignore-path ../../../.prettierignore",
		"ide-ext:package": "vsce package --yarn --failOnIssues",
		"madge": "madge --warning --circular --extensions ts ./"
	},
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"configuration": {
			"id": "sap.ux.help",
			"type": "object",
			"title": "%configuration.title%",
			"properties": {
				"sap.ux.help.enableTelemetry": {
					"default": true,
					"type": "boolean",
					"description": "%configuration.properties.enableTelemetry.description%",
					"scope": "window",
					"markdownDeprecationMessage": "%configuration.properties.deprecation.enableTelemetry.description%",
					"deprecationMessage": "%configuration.properties.deprecation.enableTelemetry.description%"
				},
				"sap.ux.help.guideDetailsMode": {
					"type": "string",
					"default": "wizard",
					"description": "%configuration.properties.guideDetailsMode.description%",
					"enum": [
						"wizard",
						"full"
					],
					"enumDescriptions": [
						"%configuration.properties.guideDetailsMode.enum.wizard.description%",
						"%configuration.properties.guideDetailsMode.enum.full.description%"
					],
					"scope": "window"
				},
				"sap.ux.help.showDescriptions": {
					"type": "string",
					"default": "enabled",
					"description": "%configuration.properties.showDescriptions.description%",
					"enum": [
						"disabled",
						"enabled"
					],
					"enumDescriptions": [
						"%configuration.properties.showDescriptions.enum.disabled.description%",
						"%configuration.properties.showDescriptions.enum.enabled.description%"
					],
					"scope": "window"
				},
				"sap.ux.help.loggingLevel": {
					"type": "string",
					"default": "information",
					"description": "%configuration.properties.loggingLevel.description%",
					"enum": [
						"information",
						"warning",
						"error",
						"debug"
					],
					"enumDescriptions": [
						"%configuration.properties.loggingLevel.enum.info.description%",
						"%configuration.properties.loggingLevel.enum.warn.description%",
						"%configuration.properties.loggingLevel.enum.error.description%",
						"%configuration.properties.loggingLevel.enum.debug.description%"
					],
					"scope": "window"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sap-ux-help-fiori-ai",
					"title": "%viewsContainers.activitybar.title%",
					"icon": "media/fiori-tools-ai.svg"
				}
			]
		},
		"views": {
			"sap-ux-help-fiori-ai": [
				{
					"id": "sap-ux-help-fiori-ai.chatProjectExplorerView",
					"when": "showBetaFeatures",
					"name": "%views.sap.ux.help.fiori.ai.chatProjectExplorerView.name%"
				},
				{
					"type": "webview",
					"id": "sap-ux-help-fiori-ai.chatView",
					"when": "showBetaFeatures",
					"name": "%views.sap.ux.help.fiori.ai.chatView.name%"
				}
			]
		},
		"commands": [
			{
				"command": "sap.ux.help.openGuidedDevelopment",
				"title": "%commands.help.openGuidedDevelopment.title%",
				"category": "%commands.help.category%"
			},
			{
				"command": "sap.ux.help.openGuidedDevelopmentToTheSide",
				"title": "%commands.help.openGuidedDevelopmentToTheSide.title%",
				"category": "%commands.help.category%"
			},
			{
				"command": "sap.ux.help.openGuidedDevelopmentFromExplorer",
				"title": "%commands.help.openGuidedDevelopmentFromExplorer.title%",
				"category": "%commands.help.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatProjectExplorerView.openFile",
				"title": "%commands.help.ai.chatProjectExplorerView.openFile.title%",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatProjectExplorerView.refreshFile",
				"title": "%commands.help.ai.chatProjectExplorerView.refreshFile.title%",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatView.clearChatMessages",
				"title": "%commands.help.ai.chatView.clearChatMessages.title%",
				"icon": "$(clear-all)",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatView.exportChatMessages",
				"title": "%commands.help.ai.chatView.exportChatMessages.title%",
				"icon": "$(cloud-download)",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatView.openReadMe",
				"title": "%commands.help.ai.chatView.openReadMe.title%",
				"icon": "$(info)",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.createProject",
				"title": "%commands.help.ai.createProject.title%",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.storeServiceKey",
				"title": "%commands.help.ai.storeServiceKey.title%",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.deleteServiceKey",
				"title": "%commands.help.ai.deleteServiceKey.title%",
				"category": "%commands.help.ai.category%"
			},
			{
				"command": "sap.ux.help.fiori.ai.chatView.displayLLMProviderSettings",
				"title": "%commands.help.ai.chatView.displayLLMProviderSettings.title%",
				"category": "%commands.help.ai.category%"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourcePath in sap.ux.contextMenu.appRoots || resourcePath in sap.ux.contextMenu.webappRoots || resourcePath in sap.ux.contextMenu.subWebappRoots",
					"command": "sap.ux.help.openGuidedDevelopmentFromExplorer",
					"group": "1__fiori_tools@3"
				}
			],
			"commandPalette": [
				{
					"command": "sap.ux.help.openGuidedDevelopmentFromExplorer",
					"when": "false"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatProjectExplorerView.openFile",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatProjectExplorerView.refreshFile",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.clearChatMessages",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatProjectExplorerView.openFile",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.exportChatMessages",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.openReadMe",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.createProject",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.storeServiceKey",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.deleteServiceKey",
					"when": "showBetaFeatures"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.displayLLMProviderSettings",
					"when": "false"
				}
			],
			"view/title": [
				{
					"command": "sap.ux.help.fiori.ai.chatView.clearChatMessages",
					"when": "view == sap-ux-help-fiori-ai.chatView",
					"group": "navigation"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.exportChatMessages",
					"when": "view == sap-ux-help-fiori-ai.chatView",
					"group": "navigation"
				},
				{
					"command": "sap.ux.help.fiori.ai.chatView.openReadMe",
					"when": "view == sap-ux-help-fiori-ai.chatView",
					"group": "navigation"
				}
			]
		}
	},
	"dependencies": {
		"@sap-ux/fe-fpm-writer": "0.24.0",
		"@sap/cds": "7.0.2",
		"@sap/ux-extension-logger": "1.11.4",
		"@sap/ux-help-webapp": "1.11.4",
		"@sap/ux-specification-types": "1.108.16"
	},
	"devDependencies": {
		"@sap-devx/bas-platform-types": "0.0.10",
		"@sap-devx/guided-development-types": "0.0.12",
		"@sap-ux/cap-config-writer": "0.2.0",
		"@sap-ux/store": "0.4.0",
		"@sap-ux/vocabularies-types": "0.7.4",
		"@sap/cds-reflect": "2.13.3",
		"@sap/ux-extension-project-access": "1.11.4",
		"@sap/ux-feature-toggle": "1.11.4",
		"@sap/ux-fiori-ai-extension-lib": "0.0.2",
		"@sap/ux-fiori-annotation-api": "1.11.4",
		"@sap/ux-help-core": "1.11.4",
		"@sap/ux-help-core-types": "1.11.4",
		"@sap/ux-help-webapp-ide-extension-types": "1.11.4",
		"@sap/ux-jest-runner-vscode": "1.11.4",
		"@sap/ux-metadata": "1.11.4",
		"@sap/ux-project-access": "1.11.4",
		"@sap/ux-telemetry": "1.11.4",
		"@sapux/project-spec": "1.11.4",
		"@sapux/vsce": "1.78.2",
		"@types/extract-zip": "1.6.2",
		"@types/marked": "4.0.1",
		"@types/mustache": "0.8.32",
		"@types/normalize-path": "3.0.0",
		"@types/vscode": "1.73.1",
		"@ui5/flexibility-utils": "0.1.3",
		"@vscode/codicons": "0.0.33",
		"copyfiles": "2.1.1",
		"cross-env": "6.0.3",
		"datauri": "2.0.0",
		"express": "4.17.3",
		"extract-zip": "1.6.6",
		"fast-glob": "3.2.12",
		"fast-safe-stringify": "2.0.7",
		"i18next": "19.0.2",
		"jest-sonar": "0.2.12",
		"jest-text-transformer": "1.0.4",
		"lodash": "4.17.21",
		"lodash.unset": "4.5.2",
		"marked": "4.0.10",
		"mustache": "3.1.0",
		"normalize-path": "3.0.0",
		"semver": "7.5.3",
		"typescript": "5.2.2"
	},
	"files": [
		"*.vsix",
		"CHANGELOG.md"
	],
	"jestSonar": {
		"reportFile": "test-report.xml",
		"reportPath": "reports/test/unit"
	},
	"galleryBanner": {
		"color": "#00195A",
		"theme": "dark"
	},
	"eslint-formatter-multiple": {
		"formatters": [
			{
				"name": "stylish",
				"output": "console"
			},
			{
				"name": "json",
				"output": "file",
				"path": "reports/lint/eslint.json"
			},
			{
				"name": "checkstyle",
				"output": "file",
				"path": "reports/lint/eslint.checkstyle.xml"
			}
		]
	},
	"__metadata": {
		"id": "9a1a59dc-6ef7-49cf-a5d0-98e64208a84f",
		"publisherId": "a17c53d7-5315-4db5-a571-feab995abf60",
		"publisherDisplayName": "SAP SE",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"updated": true,
		"isPreReleaseVersion": false,
		"installedTimestamp": 1699721760787,
		"preRelease": false
	}
}