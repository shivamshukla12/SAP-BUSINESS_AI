{
	"name": "sap-ux-service-modeler-extension",
	"displayName": "%serviceModeler.displayName%",
	"description": "%serviceModeler.description%",
	"icon": "media/Service-Modeler.png",
	"version": "1.11.4",
	"publisher": "SAPSE",
	"bugs": {
		"url": "https://ask.sapfioritools.com"
	},
	"repository": {
		"type": "git",
		"url": "private"
	},
	"engines": {
		"vscode": "^1.73.1"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"SAP",
		"SAP Fiori Elements"
	],
	"main": "./dist/extension-min.js",
	"private": false,
	"scripts": {
		"pre-commit": "lint-staged --quiet",
		"clean:dist": "rimraf ./dist ./test/int/dist ./generators *.tsbuildinfo",
		"clean": "rimraf ./test/unit/src ./out ./reports *.tgz *.vsix .webpack_cache .webpack-extension_cache",
		"build": "npm-run-all -l -s clean build:clean  -p build:extensionTS build:integrationTS build:extension build:webview build:copyfiles build:sample-data-copy build:copyTemplates",
		"build:clean": "rimraf ./dist ./out ./reports *.tgz *.vsix ./src/*.json",
		"build:extensionTS": "tsc --build ./ --pretty  --dry",
		"build:integrationTS": "tsc --build ./test/int/tsconfig.test.json --pretty",
		"build:extension": "node esbuild-extension.js",
		"build:copyfiles": "copyfiles ./src/commands/icons/* ./dist/",
		"build:copyTemplates": "copyfiles -f -V ../core/resources/templates/* ./dist/resources/templates/",
		"build:webview": "node esbuild-webapp.js",
		"build:sample-data-copy": "copyfiles --all --error -u 1 \"test/sample/**/*.{xml,csn,cds,json,js,html,yaml,md,properties,npmrc}\" dist/test",
		"build-disabled:sample-data-install": "pwd && cd dist/test/sample/cloud-cap-samples/ && npm install --package-lock=false --registry=https://int.repositories.cloud.sap/artifactory/api/npm/build-milestones-npm/ && cd ../../../../",
		"vscode:prepublish": "yarn run build",
		"watch": "npm-run-all -l -p watch:*",
		"watch:extensionTS": "tsc --build ./ --pretty --watch",
		"watch:extension": "node esbuild-extension.js --watch --minify=false --sourcemap=inline",
		"watch:copyfiles": "copyfiles ./src/commands/icons/* ./dist/",
		"watch:copyTemplates": "copyfiles -f -V ../core/resources/templates/* ./dist/resources/templates/",
		"watch:webview": "node esbuild-webapp.js --watch --minify=false  --sourcemap=inline",
		"watch:sample-data": "copyfiles --all --error --verbose -u 1 \"test/sample/**/*.{xml,csn,cds,json,js,html,yaml,md}\" dist/test",
		"test:server": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --config='jest-server.config.js'",
		"test:webview": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --config='jest-webview.config.js'",
		"test:webview:debug": "node --inspect-brk node_modules/.bin/jest --runInBand  --config='jest-webview.config.js'",
		"test": "npm-run-all -l test:unit",
		"test:unit": "cross-env SAP_UX_FIORI_TOOLS_DISABLE_TELEMETRY=true UX_TOOLS_DISABLE_KEYSTORE=true jest --maxWorkers=1 --silent --config='jest.config.js'",
		"git-status-index": "run-script-os",
		"git-status-index:default": "git status -u",
		"git-status-index:linux": "git status -u > /dev/null",
		"clean-yarn-cache": "rimraf ../../../.yarn-cache",
		"test-integration-screenshot": "run-script-os",
		"test-integration-screenshot:default": "exit 1",
		"test-integration-screenshot:linux": "cross-env DISPLAY=:99.0 && mkdirp test/reports && import -window root test/reports/final-xvfb.png && exit 1",
		"test:integration:run": "cross-env CODE_VERBOSE=true CODE_VERSION=1.77.3 CODE_FORCE_DOWNLOAD=true CODE_WAIT=true CODE_TESTS_WORKSPACE=../core/test/input/sample.Annotation.Project CODE_TESTS_PATH=./test/int/dist/ TOOL_SUITE_TIMEOUT=300000 CODE_DISABLE_EXTENSIONS=true TOOLSUITE_FEATURES=sap.ux.serviceModeler.testBetaFeatures.enableEditMode node ./../../tools/jest-runner-vscode/dist/test.js  || npm-run-all -l test-integration-screenshot",
		"test:integration": "npm-run-all -l clean-yarn-cache git-status-index test:integration:run",
		"updateSnapshot": "jest --updateSnapshot",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:summary": "eslint . --ext .ts,.tsx -f summary",
		"lint:fix": "eslint --fix . --ext .ts,.tsx",
		"lint:fix:all": "eslint . --ext .ts,.tsx --fix",
		"lint:report": "eslint . --ext .ts,.tsx -f multiple ",
		"lint:html": "eslint . --ext .ts,.tsx -o reports/lint/eslint.html -f html-extended",
		"format:fix": "prettier --write --loglevel silent --ignore-path ../../../.prettierignore",
		"format:fix:all": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,dist,typings,node_modules}/**' '!**/*.{svg,png,xml}' --ignore-path ../../../.prettierignore",
		"dev": "npm-run-all -s dev:clean dev:ext:* dev:webview",
		"dev:clean": "rimraf ./dist",
		"dev:ext:extensionTS": "tsc --build ./ --pretty ",
		"dev:ext:extension": "node esbuild-extension.js --minify=false",
		"dev:ext:copyfiles": "copyfiles ./src/commands/icons/* ./dist/",
		"dev:webview": "node esbuild-webapp.js --minify=false",
		"ide-ext:package": "vsce package --yarn --failOnIssues",
		"madge": "madge --warning --circular --extensions ts ./ || gulp show-madge-banner"
	},
	"license": "SEE LICENSE IN LICENSE",
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "sap.ux.serviceModeler.openFile",
				"title": "%commands.serviceModeler.title%",
				"category": "%commands.fiori.category%",
				"icon": {
					"light": "./dist/src/commands/icons/fileicon_light.svg",
					"dark": "./dist/src/commands/icons/fileicon_dark.svg"
				}
			},
			{
				"command": "sap.ux.serviceModeler.openFileToTheSide",
				"title": "%commands.serviceModeler.openFileToSide.title%",
				"category": "%commands.fiori.category%"
			},
			{
				"command": "sap.ux.serviceModeler.openFileCmdPalatte",
				"title": "%commands.serviceModeler.title%",
				"category": "%commands.fiori.category%"
			},
			{
				"command": "sap.ux.serviceModeler.reloadSrvModelerWebPanel",
				"title": "%commands.serviceModeler.reLoadServiceModeler.title%",
				"category": "%commands.fiori.category%"
			},
			{
				"command": "sap.ux.serviceModeler.openServiceMgr",
				"title": "%commands.serviceModeler.openServiceMgr.title%",
				"category": "%commands.fiori.category%"
			},
			{
				"command": "sap.ux.serviceModeler.openAnnotationFileMgr",
				"title": "%commands.serviceModeler.openAnnotationFileMgr.title%",
				"category": "%commands.fiori.category%"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"when": "false",
					"command": "sap.ux.serviceModeler.openFile"
				},
				{
					"when": "config.ServiceModeler.hiddenCommand == true",
					"command": "sap.ux.serviceModeler.reloadSrvModelerWebPanel"
				}
			],
			"editor/title": [
				{
					"when": "resourceExtname =~ /(xml)/ && editorTextFocus",
					"command": "sap.ux.serviceModeler.openFile",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"when": "resourceExtname =~ /(xml|edmx)/ && resourceDirname in sap.ux.contextMenu.localServiceRoots || resourceExtname =~ /(xml|edmx)/ && resourceDirname in sap.ux.contextMenu.localAnnotationRoots || resourcePath in sap.ux.contextMenu.localServiceRoots || resourcePath in sap.ux.contextMenu.projectRoots || resourcePath in sap.ux.contextMenu.appRoots || resourcePath in sap.ux.contextMenu.webappRoots",
					"command": "sap.ux.serviceModeler.openFile",
					"group": "1__fiori_tools@5"
				},
				{
					"when": "resourceFilename == manifest.json",
					"command": "sap.ux.serviceModeler.openServiceMgr",
					"group": "1__fiori_tools"
				},
				{
					"when": "resourceFilename == manifest.json",
					"command": "sap.ux.serviceModeler.openAnnotationFileMgr",
					"group": "1__fiori_tools"
				}
			]
		},
		"configuration": {
			"id": "sap.ux.serviceModeler",
			"type": "object",
			"title": "%serviceModeler.displayName%",
			"properties": {
				"sap.ux.serviceModeler.displayServiceLinksInCDS": {
					"type": "boolean",
					"description": "%configuration.properties.displayServiceLinksInCDS%",
					"default": true,
					"scope": "window"
				},
				"sap.ux.serviceModeler.displayServiceLinksInEDMX": {
					"type": "boolean",
					"description": "%configuration.properties.displayServiceLinksInEDMX%",
					"default": true,
					"scope": "window"
				},
				"sap.ux.serviceModeler.selectDefaultView": {
					"type": "string",
					"default": "List",
					"enum": [
						"%configuration.properties.selectDefaultView.enum.compact%",
						"%configuration.properties.selectDefaultView.enum.expanded%",
						"%configuration.properties.selectDefaultView.enum.list%"
					],
					"enumDescriptions": [
						"%configuration.properties.selectDefaultView.enum.compact.desc%",
						"%configuration.properties.selectDefaultView.enum.expanded.desc%",
						"%configuration.properties.selectDefaultView.enum.list.desc%"
					],
					"scope": "window"
				},
				"sap.ux.serviceModeler.enableTelemetry": {
					"default": true,
					"type": "boolean",
					"description": "%configuration.properties.enableTelemetry%",
					"scope": "window",
					"markdownDeprecationMessage": "%configuration.properties.deprecation.enableTelemetry%",
					"deprecationMessage": "%configuration.properties.deprecation.enableTelemetry%"
				}
			}
		}
	},
